{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-hooks",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "trailing-comma": true,
    "react-hooks-nesting": "error"
  },
  "rulesDirectory": []
}
